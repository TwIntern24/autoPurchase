QCalendarWidget {
    background-color: #22362B;
    color: #F2F2F2;
    border: 1px solid #345244;
}

/* Top navigation bar (month/year row) */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #2B4A3A;
    color: #F2F2F2;
}

/* Month/year buttons + arrows */
QCalendarWidget QToolButton {
    background-color: #345244;
    color: #F2F2F2;
    border: 1px solid #3B5E4F;
    border-radius: 6px;
    padding: 4px 8px;
}
QCalendarWidget QToolButton:hover {
    background-color: #3E6A59;
}

/* Month dropdown / year edit */
QCalendarWidget QMenu {
    background-color: #22362B;
    color: #F2F2F2;
    border: 1px solid #345244;
}
QCalendarWidget QSpinBox {
    background-color: #22362B;
    color: #F2F2F2;
    border: 1px solid #345244;
    border-radius: 6px;
    padding: 2px 6px;
}

/* Weekday header row (Sun, Mon, ...) */
QCalendarWidget QTableView {
    background-color: #22362B;
    alternate-background-color: #243F31;
    gridline-color: #345244;
}
QCalendarWidget QHeaderView::section {
    background-color: #2F5C48;
    color: #F2F2F2;          /* this removes red/blue if it was coming from header */
    padding: 4px;
    border: 0px;
    font-weight: 600;
}

/* Day cells */
QCalendarWidget QAbstractItemView {
    background-color: #22362B;
    color: #F2F2F2;          /* this is key: ensures normal text is not inherited oddly */
    selection-background-color: #C9A227;
    selection-color: #1B1B1B;
    outline: 0;
}

/* Optional: make weekends NOT red/blue */
QCalendarWidget QAbstractItemView::item {
    color: #F2F2F2;
}

/* Optional: “today” cell subtle border */
QCalendarWidget QAbstractItemView::item:focus {
    border: 1px solid #C9A227;
}
